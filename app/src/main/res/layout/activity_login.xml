<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginVM" type="com.uptrain.app.modules.login.data.viewmodel.LoginVM"/>
    </data>
    <FrameLayout android:id="@+id/frameLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStackthirtytwo"
            android:layout_gravity="top|start"
            android:layout_height="@dimen/_672pxv"
            android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageImageOne"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image1"/>
            <ImageView android:id="@+id/imageThirtyTwo"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_196pxv"
                android:layout_marginBottom="@dimen/_24pxv"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY" imageRes="@{@drawable/img_32}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_32"/>
        </FrameLayout>
        <LinearLayout android:id="@+id/linearColumnngnhpfaceb"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_2">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowngnhpfaceb"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_51pxh"
                android:layout_marginStart="@dimen/_51pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleindigo_701cornerRadius">
                <ImageView android:id="@+id/imageImageTwo"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_image2"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNgnhpFaceb"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtNgnhpFaceb}"
                    style="@style/txtPoppinsmedium14_1" tools:text="@string/msg_ng_nh_p_faceb"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowngnhpgoogl"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_51pxh"
                android:layout_marginStart="@dimen/_51pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleindigo_701cornerRadius">
                <ImageView android:id="@+id/imageImageThree"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_image3"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image3"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNgnhpGoogl"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_47pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.87"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtNgnhpGoogl}"
                    style="@style/txtPoppinsmedium14_1" tools:text="@string/msg_ng_nh_p_googl"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowngnhptik"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_51pxh"
                android:layout_marginStart="@dimen/_51pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleindigo_701cornerRadius">
                <ImageView android:id="@+id/imageImageFour"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_image4"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image4"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNgnhptik"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtNgnhptik}"
                    style="@style/txtPoppinsmedium14_1" tools:text="@string/msg_ng_nh_p_t_i_k"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowbnchacti"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_28pxv"
                android:layout_marginEnd="@dimen/_61pxh"
                android:layout_marginStart="@dimen/_61pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBnchacti"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.75"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtBnchacti}"
                    style="@style/txtPoppinsmediumitalic14" tools:text="@string/msg_b_n_ch_a_c_t_i"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNgk"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.25"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtNgk}"
                    style="@style/txtPoppinsmediumitalic14_1" tools:text="@string/lbl_ng_k2"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
